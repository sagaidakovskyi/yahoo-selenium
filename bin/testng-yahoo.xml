<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Yahoo Selenium WebDriver Tests" verbose="10">

    <parameter name="browser" value="chrome"/>
    <parameter name="yahoo_user_name" value="java.selenium"/>
    <parameter name="yahoo_user_password" value="securePWDforJAVA"/>

    <test name="Login/Logout" verbose="10">
        <classes>
            <class name="com.msagaidakovskyi.yahoo.project.tests.LoginTest"/>
        </classes>
    </test>
</suite>